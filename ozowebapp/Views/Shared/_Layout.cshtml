<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - ozowebapp</title>


    <link href="https://fonts.googleapis.com/css?family=Roboto:100,300,400,500,700" rel="stylesheet">

    <!-- Animate.css -->
    <link href="~/css/layout/animate.css" rel="stylesheet">
    <!-- Icomoon Icon Fonts-->
    <link href="~/css/layout/icomoon.css" rel="stylesheet">
    <!-- Icons -->
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.7.0/css/font-awesome.min.css">
    <script src='https://kit.fontawesome.com/a076d05399.js'></script>
    <!-- Bootstrap  -->
    <link href="~/css/layout/bootstrap.css" rel="stylesheet">
    <!-- Flexslider  -->
    <link href="~/css/layout/flexslider.css" rel="stylesheet">
    <!-- Theme style  -->
    <link href="~/css/layout/style.css" rel="stylesheet">

    <!-- Modernizr JS -->
    <script src="~/js/layout/modernizr-2.6.2.min.js" defer></script>

</head>
<body>
    <div id="fh5co-page">
        <a href="#" class="js-fh5co-nav-toggle fh5co-nav-toggle"><i></i></a>
        <aside id="fh5co-aside" role="complementary" class="border js-fullheight">
            <nav id="fh5co-main-menu" role="navigation">
                <ul>
                    <li><a class="nav-link text-dark" asp-area="" asp-controller="Home" asp-action="Index">Naslovnica</a></li>
                    <li><a class="nav-link text-dark" asp-area="" asp-controller="Home" asp-action="Privacy">Privacy view</a></li>
                    <li><a class="nav-link text-dark" asp-area="" asp-controller="Djelatnici" asp-action="Index">Djelatnici</a></li>
                    <li><a class="nav-link text-dark" asp-area="" asp-controller="Zanimanja" asp-action="Index">Zanimanja</a></li>
                    <li><a class="nav-link text-dark" asp-area="" asp-controller="Oprema" asp-action="Index">Oprema</a></li>
                    <hr>
                </ul>
            </nav>
        </aside>
    </div>
    <div class="container">
        <main role="main" class="pb-3">
            @RenderBody()
        </main>
    </div>

    <!-- jQuery -->
    <script src="~/js/layout/jquery.min.js"></script>
    <!-- jQuery Easing -->
    <script src="~/js/layout/jquery.easing.1.3.js" defer></script>
    <!-- Bootstrap -->
    <script src="~/js/layout/bootstrap.min.js" defer></script>
    <!-- Waypoints -->
    <script src="~/js/layout/jquery.waypoints.min.js" defer></script>
    <!-- Flexslider -->
    <script src="~/js/layout/jquery.flexslider-min.js" defer></script>

    <!-- MAIN JS -->
    <script src="~/js/layout/main.js" defer></script>
    @RenderSection("Scripts", required: false)
</body>
</html>
